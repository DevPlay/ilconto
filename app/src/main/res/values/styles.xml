<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#ffa726</item>
        <item name="colorPrimaryDark">#FFF3E0</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:colorBackground">@color/Barra_estado</item>
        <item name="android:statusBarColor">@color/statusBarColor</item>
        <item name="android:colorForeground">@android:color/transparent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">#ffa726</item>
        <item name="colorPrimaryDark">#ffa726</item>
        <item name="colorAccent">#ffa726</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Ilconto" parent="AppTheme" >
        <item name="colorPrimary">@color/colorAccent</item>
        <item name="colorAccent">@color/Barra_estado</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="android:textColorSecondaryInverse">@android:color/darker_gray</item>
        <item name="android:textColorSecondary">@android:color/tab_indicator_text</item>
    </style>

</resources>
