<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#ffa726</item>
        <item name="colorPrimaryDark">#FFF3E0</item>
        <item name="colorAccent">?attr/colorBackgroundFloating</item>
        <item name="android:colorBackground">@color/Barra_estado</item>
        <item name="android:navigationBarColor">@android:color/darker_gray</item>
        <item name="android:statusBarColor">@color/Barra_estado</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">#ffa726</item>
        <item name="colorPrimaryDark">#ffa726</item>
        <item name="colorAccent">#ffa726</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
